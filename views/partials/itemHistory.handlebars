<section class = "history-item">
  <section class="history-date"><span class="date">Date: </span>{{date}}</section>
  <section class="history-consumption"><span class="consumption-amount">Quantity: </span>{{quantity}} {{unit}}</section>
  <button type="button" id="{{id}}" class="history-remove-button">&times;</button>
</section>
